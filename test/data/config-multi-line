macro p open;\
reload;\
quit;\
quit -- "opens, reloads, quits, then quits newsboat"

macro j qui\
t;\
qu\
it;\
q\
uit;\
quit -- "Dummy macro"


macro f open;  \
reload;        \
quit;          \
quit  -- "Spaces before \ are ok. Not after, if concatenation is what the user is after. Otherwise treated as-is"

macro a open; quit; reload -- "some macro description"
